web: gunicorn -w $WEB_WORKERS --worker-class eventlet -b 0.0.0.0:$PORT app:app
default_worker: celery worker --app=tasks:celery --concurrency $CELERY_WORKERS -Q default -l info
fetch_detail_worker: celery worker --app=tasks:celery --concurrency $CELERY_WORKERS -Q fetch_detail -l info
beat: celery beat --app=tasks:celery
